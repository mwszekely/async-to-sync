{
  "name": "async-to-sync",
  "type": "module",
  "version": "0.0.1",
  "description": "Turns an async function into a sync function (+ a lot of callbacks and other things)",
  "source": "src/index.ts",
  "module": "/dist/index.js",
  "types": "/dist/index.d.ts",
  "packageManager": "pnpm@10.15.1",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core.d.ts",
      "browser": "./dist/core.js",
      "import": "./dist/core.js"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "browser": "./dist/react.js",
      "import": "./dist/react.js"
    },
    "./preact": {
      "types": "./dist/preact.d.ts",
      "browser": "./dist/preact.js",
      "import": "./dist/preact.js"
    },
    "./solid": {
      "types": "./dist/solid.d.ts",
      "browser": "./dist/solid.js",
      "import": "./dist/solid.js"
    },
    "./signals": {
      "types": "./dist/signals.d.ts",
      "browser": "./dist/signals.js",
      "import": "./dist/signals.js"
    },
    "./signals-react": {
      "types": "./dist/signals-react.d.ts",
      "browser": "./dist/signals-react.js",
      "import": "./dist/signals-react.js"
    },
    "./signals-preact": {
      "types": "./dist/signals-preact.d.ts",
      "browser": "./dist/signals-preact.js",
      "import": "./dist/signals-preact.js"
    },
    "./package.json": "./package.json"
  },
  "typings": "./dist/index.d.ts",
  "scripts": {
    "watch": "tsc --watch",
    "build": "tsc",
    "lint": "npx eslint ./src",
    "clean": "tsc --build --clean",
    "test": "playwright test",
    "rebuild": "rm \"./pnpm-lock.yaml\" && rm -R \"./node_modules\" && pnpm install && pnpm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "typescript": "^5.0.3"
  },
  "peerDependencies": {
    "@preact/signals": "^2.3.1",
    "react": "^19.1.1",
    "solid-js": "^1.9.9",
    "preact": "^10.27.2"
  },
  "directories": {
    "test": "tests"
  }
}
